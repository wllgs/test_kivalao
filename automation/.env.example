# PostgreSQL database used by Prisma and n8n
POSTGRES_DB=kivalao
POSTGRES_USER=kivalao
POSTGRES_PASSWORD=super-secret
POSTGRES_PORT=5432

# n8n authentication and base configuration
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=change-me
N8N_HOST=localhost
N8N_PROTOCOL=http
N8N_WEBHOOK_URL=http://localhost:5678/

# Shared configuration for webhook calls
KIVALAO_API_URL=http://localhost:3000

NOTIFICATION_FROM_EMAIL=notifications@kivalao.app
